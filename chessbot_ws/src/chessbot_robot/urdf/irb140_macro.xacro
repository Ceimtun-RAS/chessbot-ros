<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
    

<xacro:macro name="abb_irb140" params="prefix">
<!-- Worlds  -->
  <link name="world"/>
    <!-- link list -->
    <link name="${prefix}base_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://chessbot_robot/meshes/irb140/visual/base_link.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="abb_orange">
        <color rgba="1 0.43 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://chessbot_robot/meshes/irb140/collision/base_link.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="yellow">
        <color rgba="0 1 1 1"/>
      </material>
    </collision>
    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <mass value="1.0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    
  </link>
  <link name="${prefix}link_1">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://chessbot_robot/meshes/irb140/visual/link_1.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="abb_orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://chessbot_robot/meshes/irb140/collision/link_1.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="yellow"/>
    </collision>
     <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <mass value="1.0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
  </link>
  <link name="${prefix}link_2">
    <visual>
      <origin rpy="0 0 0" xyz="-0.07 0 -0.352"/>
      <geometry>
        <mesh filename="package://chessbot_robot/meshes/irb140/visual/link_2.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="abb_orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="-0.07 0 -0.352"/>
      <geometry>
        <mesh filename="package://chessbot_robot/meshes/irb140/collision/link_2.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="yellow"/>
    </collision>
     <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <mass value="1.0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
  </link>
  <link name="${prefix}link_3">
    <visual>
      <origin rpy="0 0 0" xyz="-0.07 0.065 -0.712"/>
      <geometry>
        <mesh filename="package://chessbot_robot/meshes/irb140/visual/link_3.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="abb_orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="-0.07 0.065 -0.712"/>
      <geometry>
        <mesh filename="package://chessbot_robot/meshes/irb140/collision/link_3.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="yellow"/>
    </collision>
     <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <mass value="1.0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
  </link>
  <link name="${prefix}link_4">
    <visual>
      <origin rpy="0 0 0" xyz="-0.309 0 -0.712"/>
      <geometry>
        <mesh filename="package://chessbot_robot/meshes/irb140/visual/link_4.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="abb_orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="-0.309 0 -0.712"/>
      <geometry>
        <mesh filename="package://chessbot_robot/meshes/irb140/collision/link_4.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="yellow"/>
    </collision>
     <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <mass value="1.0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
  </link>
  <link name="${prefix}link_5">
    <visual>
      <origin rpy="0 0 0" xyz="-0.450 0 -0.712"/>
      <geometry>
        <mesh filename="package://chessbot_robot/meshes/irb140/visual/link_5.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="abb_orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="-0.450 0 -0.712"/>
      <geometry>
        <mesh filename="package://chessbot_robot/meshes/irb140/collision/link_5.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="yellow"/>
    </collision>
     <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <mass value="1.0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
  </link>
  <link name="${prefix}link_6">
    <visual>
      <origin rpy="0 0 0" xyz="-0.515 0 -0.712"/>
      <geometry>
        <mesh filename="package://chessbot_robot/meshes/irb140/visual/link_6.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="abb_orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="-0.515 0 -0.712"/>
      <geometry>
        <mesh filename="package://chessbot_robot/meshes/irb140/collision/link_6.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="yellow"/>
    </collision>
     <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <mass value="1.0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
  </link>

  <link name="${prefix}tool0"/>


  
    <!-- end of link list -->
  <joint name="fixed" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
  </joint>

    <!-- joint list -->
   <joint name="${prefix}joint_1" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="${prefix}base_link"/>
    <child link="${prefix}link_1"/>
    <axis xyz="0 0 1"/>
    <limit effort="0" lower="-3.1416" upper="3.1416" velocity="2.618"/>
  </joint>

  <joint name="joint_2" type="revolute">
    <origin rpy="0 0 0" xyz="0.070 0 0.352"/>
    <parent link="${prefix}link_1"/>
    <child link="${prefix}link_2"/>
    <axis xyz="0 1 0"/>
    <limit effort="0" lower="-1.7453" upper="1.7453" velocity="2.618"/>
  </joint>

  <joint name="joint_3" type="revolute">
    <origin rpy="0 0 0" xyz="0 -0.065 0.360"/>
    <parent link="${prefix}link_2"/>
    <child link="${prefix}link_3"/>
    <axis xyz="0 1 0"/>
    <limit effort="0" lower="-2.4434" upper="2.4434" velocity="2.618"/>
  </joint>

  <joint name="${prefix}joint_4" type="revolute">
    <origin rpy="0 0 0" xyz="0.239 0.065 0"/>
    <parent link="${prefix}link_3"/>
    <child link="${prefix}link_4"/>
    <axis xyz="1 0 0"/>
    <limit effort="0" lower="-3.49" upper="3.49" velocity="6.2832"/>
  </joint>

  <joint name="${prefix}joint_5" type="revolute">
    <origin rpy="0 0 0" xyz="0.141 0 0"/>
    <parent link="${prefix}link_4"/>
    <child link="${prefix}link_5"/>
    <axis xyz="0 1 0"/>
    <limit effort="0" lower="-2.0071" upper="2.0071" velocity="6.2832"/>
  </joint>

  <joint name="${prefix}joint_6" type="revolute">
    <origin rpy="0 0 0" xyz="0.065 0 0"/>
    <parent link="${prefix}link_5"/>
    <child link="${prefix}link_6"/>
    <axis xyz="1 0 0"/>
    <limit effort="0" lower="-6.9813" upper="6.9813" velocity="7.854"/>
  </joint>
  
  <joint name="${prefix}joint_6-tool0" type="fixed">
    <parent link="${prefix}link_6"/>
    <child link="${prefix}tool0"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>
    <!-- end of joint list -->

    <!-- ROS base_link to ABB World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="${prefix}base_link"/>
        <child link="${prefix}base"/>
    </joint>
</xacro:macro>
</robot>
