<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find chessbot_controller)/config.yaml" command="load"/>
  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen"  ns = "/chessbot" args="joint1_position_controller joint2_position_controller joint3_position_controller joint_state_controller"/>

